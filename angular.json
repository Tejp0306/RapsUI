{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "myNewApp": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/my-new-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/img"
              
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "src/styles.css",
              "src/css/globals.css",
              "src/css/rapscorp-landing-2.css",
              "src/css/styleguide.css",
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "node_modules/font-awesome/css/font-awesome.css"
           
               
            ],
            "scripts": ["./node_modules/bootstrap/dist/js/bootstrap.min.js",
                           "./node_modules/jquery/dist/jquery.min.js",
                           "./node_modules/@lottiefiles/lottie-player/dist/lottie-player.js"     ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "myNewApp:build:production"
            },
            "development": {
              "browserTarget": "myNewApp:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "myNewApp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/img",
              {
                "glob": "*.svg",
                "input": "./node_modules/bootstrap-icons/",
                "output": "/"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": ["./node_modules/bootstrap/dist/js/bootstrap.min.js",
            "../node_modules/jquery/dist/jquery.min.js",
            "./node_modules/@lottiefiles/lottie-player/dist/lottie-player.js"     ]
          }
        }
      }
    }
  }
}
